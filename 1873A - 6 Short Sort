#include <bits/stdc++.h>
using namespace std;

int main(){
    int t;
    cin >> t;
    
    while(t--){
        char arr[3];
        for (int i = 0;i<3;i++){
        	cin >> arr[i];
        }
        if (arr[0] == 'a' && arr[1] == 'b' && arr[2] == 'c'){
            cout << "YES\n";        }
        else if (arr[0] == 'a' && arr[1] == 'c' && arr[2] == 'b'){
            cout << "YES\n";
        }
        else if (arr[0] == 'a' && arr[1] == 'c' && arr[2] == 'b'){
            cout << "YES\n";
        }
        else if (arr[0] == 'c' && arr[1] == 'b' && arr[2] == 'a'){
            cout << "YES\n";
        }
        else if (arr[0] == 'b' && arr[1] == 'a' && arr[2] == 'c'){
            cout << "YES\n";
        }
        else{
            cout << "NO\n";
        }
    }
}
