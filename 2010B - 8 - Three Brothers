#include <bits/stdc++.h>
using namespace std;

int main(){\
    int m, n;
    cin >> m >> n;
    if (m == 1){
        if (n==2){
            cout << "3";
        }
        else if (n == 3){
            cout << "2";
        }
    }
    if (m == 2){
        if (n==1){
            cout << "3";
        }
        else if (n == 3){
            cout << "1";
        }
    }
    if (m == 3){
        if (n==1){
            cout << "2";
        }
        else if (n == 2){
            cout << "1";
        }
    }
    
    return 0;
}
